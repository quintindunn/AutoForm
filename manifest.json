{    "manifest_version": 3,
    "name": "FormFill",
    "version": "0.0.1",
    "description": "Refill Google Form",
    "action": {
        "default_title": "Form Filler",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "offline_enabled": true,
    "permissions": [
        "webRequest",
        "storage"

    ],
	"host_permissions": [
		"https://docs.google.com/*",
        "https://forms.gle/*",
		"https://mrgrodskichemistry.com/*"
	]
}